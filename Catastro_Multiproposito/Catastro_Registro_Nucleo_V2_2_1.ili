CONTRACTED MODEL Catastro_Registro_Nucleo_V2_2_1 (es)
AT "http://www.proadmintierra.info/"
VERSION "V2.2.1"  // 2018-02-19 // =
  IMPORTS ISO19107_V1_MAGNABOG,LADM_COL_V1_1;

  DOMAIN

    /** !! ajustar en el Catalogo de Objetos
     */
    COL_AcuerdoTipo = (
      /** Existe un acuerdo sobre la posición del punto
       */
      Acuerdo,
      /** Existe un desacuerdo sobre la posición del punto
       */
      Desacuerdo
    );

    COL_Afectacion = (
      Inundacion,
      /** Remocion en Masa
       */
      RemocionMasa,
      Otra
    );

    COL_BosqueAreaSemi = (
      /** Área Boscosa
       */
      AreaBoscosa,
      /** Plantación Forestal
       */
      PlantaForestal
    );

    COL_CuerpoAgua = (
      NacimientoAgua,
      /** Cuerpo de agua natural o artificial
       */
      CuerpoAgua,
      ZonaPantanosa
    );

    COL_DerechoTipo = (
      Derecho_Propiedad_Colectiva,
      Mineria_Derecho,
      Nuda_Propiedad,
      Posesion_Ocupacion,
      Tenencia,
      Usufructo,
      /** Derecho de dominio o propiedad
       */
      Dominio
    );

    COL_EstructuraTipo = (
      Croquis,
      Linea_no_Estructurada,
      Texto,
      Topologico
    );

    COL_ExplotacionTipo = (
      Minera,
      Hidrocarburo,
      Otra
    );

    COL_GeneroTipo = (
      Femenino,
      Masculino,
      Otro
    );

    COL_InteresadoDocumentoTipo = (
      Cedula_Ciudadania,
      Cedula_Extranjeria,
      NIT,
      Pasaporte,
      Tarjeta_Identidad,
      Libreta_Militar,
      Registro_Civil,
      Cedula_Militar,
      NUIP,
      Secuencial_SNR,
      Secuencial_IGAC
    );

    COL_MonumentacionTipo = (
      Incrustacion,
      Mojon,
      No_Materializado,
      Otros,
      Pilastra
    );

    /** Tipos de predios de acuerdo a la resolucion 070 de 2011
     */
    COL_PredioTipo = (
      NPH,
      PropiedadHorizontal(
        Matriz,
        UnidadPredial
      ),
      Condominio(
        Matriz,
        UnidadPredial
      ),
      Mejora,
      ParqueCementerio(
        Matriz,
        UnidadPrivada
      ),
      Via,
      BienUsoPublico,
      Deposito,
      Parqueadero,
      Bodega
    );

    COL_ServidumbreTipo = (
      Vial,
      Petrolera,
      Electrica,
      Otra
    );

    COL_TerritorioAgricola = (
      CultTransitorio,
      CultPermanente,
      Confinado,
      /** Tierra en preparacion o descanso
       */
      TierraPrepodesc,
      AreaAgriHetero,
      Pasto
    );

    COL_UnidadEdificacionTipo = (
      Compartido,
      individual
    );

    COL_DefPuntoTipo = (
      Bien_Definido,
      No_Bien_Definido
    );

    COL_InstitucionTipo = (
      Registraduria_Nacional,
      Registro_Propiedad,
      Catastro_IGAC,
      Catastro_Descentralizado,
      URT,
      ANT
    );

    COL_InteresadoJuridicoTipo = (
      Publico,
      Privado,
      Mixto
    );

    /** Conjunto de valores para indicar si se trata de un punto de control de referencia (un punto principal) o de apoyo (uso para levantamientos locales con estación total)
     */
    COL_PuntoControlTipo = (
      Control,
      Apoyo
    );

    COL_RedServiciosTipo = (
      Petroleo,
      Quimicos,
      Red_Termica,
      Telecomunicacion
    );

    COL_ResponsabilidadTipo = (
      Constitucional,
      Legal,
      Contractual,
      Administrativa,
      Judicial,
      Otros
    );

    COL_DescripcionPuntoTipo = (
      Esquina_Construccion,
      Interseccion_Cerca_De_Piedra,
      Interseccion_Cerca_Viva,
      Poste_de_Cerca,
      /** !! por definir durante pilotos
       */
      Otros
    );

    /** Punto de leventamiento planimetrico que se identifican en el marco de la identificación de las construcciones, los linderos o puntos auxiliares levantado para el apoyo en la mediciión
     */
    COL_PuntoLevTipo = (
      Auxiliar,
      Construccion,
      Servidumbre
    );

    COL_RestriccionTipo = (
      Afectaciones_Interes_General,
      Ambientales,
      Desplazamiento_Forzado_Restitucion,
      Embargo,
      Hipoteca,
      Propiedad_Horizontal_y_Urbanismo,
      Prohibiciones_Expresas,
      Proteccion_Familia,
      Servidumbre,
      No_Registrada
    );

    COL_ViaTipo = (
      Arteria,
      Autopista,
      Carreteable,
      Cicloruta,
      Colectora,
      Departamental,
      Ferrea,
      Local,
      Metro_o_Metrovia,
      Nacional,
      Ordinaria,
      Peatonal,
      Principal,
      Privada,
      Secundaria,
      Troncal
    );

    COL_HipotecaTipo = (
      Abierta,
      Cerrada
    );

    /** Si ha sido situado por interpolación, de qué manera se ha hecho.
     */
    COL_InterpolacionTipo = (
      Aislado,
      Intermedio_Arco,
      Intermedio_Linea
    );

    COL_ZonaTipo = (
      Perimetro_Urbano,
      Rural,
      Corregimiento,
      Caserios,
      Inspecion_Policia
    );

    /** Dominio con la descripción de la tipologia de los codigos registrales que se inscriben y que publicitan alguna caracteristica especial del predio
     */
    COL_PublicidadTipo = (
      Demanda,
      Inicio_de_Proceso_Administrativo,
      Cancelacion,
      Desplazamiento_Forzado_Victima_o_Restitucion,
      Publicidad_de_Acto_Juridico
    );

    COL_TipoConstruccionTipo = (
      Anexo,
      No_PH,
      Parque_Cementerio,
      PH
    );
  STRUCTURE COL_Afectacion_Terreno_Afectacion = value : MANDATORY COL_Afectacion; END COL_Afectacion_Terreno_Afectacion;
  STRUCTURE COL_BosqueAreaSemi_Terreno_Bosque_Area_Seminaturale = value : MANDATORY COL_BosqueAreaSemi; END COL_BosqueAreaSemi_Terreno_Bosque_Area_Seminaturale;
  STRUCTURE COL_CuerpoAgua_Terreno_Evidencia_Cuerpo_Agua = value : MANDATORY COL_CuerpoAgua; END COL_CuerpoAgua_Terreno_Evidencia_Cuerpo_Agua;
  STRUCTURE COL_ExplotacionTipo_Terreno_Explotacion = value : MANDATORY COL_ExplotacionTipo; END COL_ExplotacionTipo_Terreno_Explotacion;
  STRUCTURE COL_ServidumbreTipo_Terreno_Servidumbre = value : MANDATORY COL_ServidumbreTipo; END COL_ServidumbreTipo_Terreno_Servidumbre;
  STRUCTURE COL_TerritorioAgricola_Terreno_Territorio_Agricola = value : MANDATORY COL_TerritorioAgricola; END COL_TerritorioAgricola_Terreno_Territorio_Agricola;

  TOPIC Catastro_Registro
  EXTENDS LADM_COL_V1_1.LADM_Nucleo =
    DEPENDS ON LADM_COL_V1_1.LADM_Nucleo;

    /** Clase que registra las instancias de los derechos que un interesado ejerce sobre un predio. Es una especialización de la clase LA_RRR del propio modelo.
     */
    CLASS COL_Derecho
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_RRR =
      /** Derecho que se ejerce.
       */
      Tipo : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_DerechoTipo;
      /** Código con el que el derecho se registra en el Registro de la Propiedad.
       */
      Codigo_Registral_Derecho : TEXT*5;
    END COL_Derecho;

    CLASS COL_Interesado
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_Interesado =
      /** Documento de identidad del interesado.
       */
      Documento_Identidad : MANDATORY TEXT*12;
      /** Tipo de documento del que se trata.
       */
      Tipo_Documento : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_InteresadoDocumentoTipo;
      /** Quien ha emitido el documento de identidad.
       */
      Organo_Emisor : TEXT*20;
      /** Fecha de emisión del documento de identidad.
       */
      Fecha_Emision : INTERLIS.XMLDate;
      /** Primer apellido de la persona física.
       */
      Primer_Apellido : MTEXT*100;
      /** Primer nombre de la persona física.
       */
      Primer_Nombre : MTEXT*100;
      /** Segundo apellido de la persona física.
       */
      Segundo_Apellido : MTEXT*100;
      /** Segundo nombre de la persona física.
       */
      Segundo_Nombre : MTEXT*100;
      /** Nombre con el que está inscrito.
       */
      Razon_Social : MTEXT*250;
      Genero : Catastro_Registro_Nucleo_V2_2_1.COL_GeneroTipo;
      Tipo_Interesado_Juridico : Catastro_Registro_Nucleo_V2_2_1.COL_InteresadoJuridicoTipo;
    END COL_Interesado;

    /** Es un tipo de espacio jurídico de la unidad de edificación del modelo LADM que almacena datos específicos del avalúo resultante del mismo.
     */
    CLASS Construccion
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_EspacioJuridicoUnidadEdificacion =
      /** Rsultado del cálculo de su avalúo mediante la metodología legalmente establecida.
       */
      Avaluo_Construccion : MANDATORY LADM_COL_V1_1.LADM_Nucleo.Peso;
      Area_Construccion : 0.0 .. 99999999999999.9;
      !!@name="Avaluo Construccion"
      !!@ilivalid.msg="El Avaluo Construccion debe ser mayor a cero"
      SET CONSTRAINT Avaluo_Construccion > 0;
      !!@name="Area Construccion"
      !!@ilivalid.msg="El Area Construccion debe ser mayor a cero"
      SET CONSTRAINT WHERE DEFINED(Area_Construccion):  
        Area_Construccion > 0;
    END Construccion;

    CLASS Interesado_Contacto =
      Telefono1 : TEXT*20;
      Telefono2 : TEXT*20;
      Domicilio_Notificacion : TEXT*500;
      Correo_Electronico : TEXT*100;
      Origen_Datos : Catastro_Registro_Nucleo_V2_2_1.COL_InstitucionTipo;
    END Interesado_Contacto;

    /** Clase especializada de LA_CadenaCarasLindero que permite registrar los linderos.
     * Dos linderos no pueden cruzarse ni superponerse.
     */
    CLASS Lindero
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_CadenaCarasLimite =
      /** Lóngitud en m del lindero.
       */
      Longitud : MANDATORY 0.0 .. 10000.0 [INTERLIS.m];
    END Lindero;

    /** Clase especializada de BaUnit, que describe la unidad administrativa básica para el caso de Colombia.
     * El predio es la unidad territorial legal propia de Catastro. Esta formada por el terreno y puede o no tener construcciones asociadas.
     */
    CLASS Predio
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_BAUnit =
      /** Corresponde al codigo del departamento al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 2 dígitos.
       */
      Departamento : TEXT*2;
      /** Corresponde al codigo del municipio al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 3 dígitos.
       */
      Municipio : TEXT*3;
      /** Corresponde a la zona castrastral, definida para optimizar las actividades catastrales. Es un codigo de 2 dígitos.
       */
      Zona : TEXT*2;
      /** Numero Unico de identificación Predial. Es el codigo definido en el proyecto de ley que será el codigo de identificación del predio tanto para catastratro como para Registro.
       */
      NUPRE : MANDATORY TEXT*20;
      /** Folio de Matricula Inmobilidaria. Codigo único de identificación asignado al documento registral en la oficina de registro de instrumentos públicos.
       */
      FMI : TEXT*80;
      /** Nuevo código númerico de treinta (30) dígitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustin Codazzi.
       */
      Numero_Predial : TEXT*30;
      /** Anterior código númerico de veinte (20) digitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustin Codazzi.
       */
      Numero_Predial_Anterior : TEXT*20;
      /** Valor de cada predio, obtenido mediante investigación y análisis estadistico del mercado inmobiliario y la metodología de aplicación  correspondiente. El avalúo  catastral de cada predio se determina a partir de la adición de los avalúos parciales practicados independientemente para los terrenos y para las edificaciones en el comprendidos.
       */
      Avaluo_Predio : LADM_COL_V1_1.LADM_Nucleo.Peso;
      Tipo (EXTENDED) : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_PredioTipo;
      UNIQUE NUPRE; 
    END Predio;

    /** Clase especial del perfil colombiano de la norma ISO 19152:2012. Pretenden generar publicidad sobre el predio a partir del almacenamiento de los codigos registrales que se inscriben el FMI. No se genera ningún tipo de derecho, ni limita la propiedad.
     */
    CLASS Publicidad
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.ObjetoVersionado =
      /** Indica la característica por la que se hace público.
       */
      Tipo : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_PublicidadTipo;
      /** Código registral del FMI que se hace público.
       */
      Codigo_Registral_Publicidad : MANDATORY TEXT*5;
      /** Identificador global único.
       */
      p_Espacio_De_Nombres : MANDATORY LADM_COL_V1_1.LADM_Nucleo.CharacterString;
      /** Identificador único local.
       */
      p_Local_Id : MANDATORY LADM_COL_V1_1.LADM_Nucleo.CharacterString;
    END Publicidad;

    /** Clase especializada de LA_Punto que representa puntos de la densificación de la red local, que se utiliza en la operación catastral para el levantamiento de información fisica de los objetos territoriales, como puntos de control.
     */
    CLASS PuntoControl
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_Punto =
      /** Nombre que recibe el punto.
       */
      Nombre_Punto : MANDATORY TEXT*20;
      /** Exactitud vertical de la medición del punto.
       */
      Exactitud_Vertical : MANDATORY 0 .. 1000 [LADM_COL_V1_1.cm];
      /** Exactitud horizontal de la medición del punto.
       */
      Exactitud_Horizontal : MANDATORY 0 .. 1000 [LADM_COL_V1_1.cm];
      /** Se describe la posición del punto con relación a su estructura, si es un punto aislado, o si hace parte de un Arco o de una linea.
       */
      Posicion_Interpolacion (EXTENDED) : Catastro_Registro_Nucleo_V2_2_1.COL_InterpolacionTipo;
      Monumentacion (EXTENDED) : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_MonumentacionTipo;
      /** Si se trata deun punto de control o de apoyo.
       */
      Tipo_Punto_Control : Catastro_Registro_Nucleo_V2_2_1.COL_PuntoControlTipo;
      /** Si el punto es o no fiable.
       */
      Confiabilidad : BOOLEAN;
    END PuntoControl;

    /** Clase especializada de LA_Punto que almacena puntos que definen un lindero, instancia de la clase LA_CadenaCarasLindero y sus especializaciones.
     */
    CLASS PuntoLindero
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_Punto =
      /** Se Indica si existe acuerdo o no entre los colindantes en relación al punto lindero que se está midiendo.
       */
      Acuerdo : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_AcuerdoTipo;
      /** Se caracteriza si el punto de levantamiento corresponde a un punto bien definido o no bien definido
       */
      Definicion_Punto : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_DefPuntoTipo;
      /** Es la descripción del tipo de punto lindero y las caracteristicas del vertice
       */
      Descripcion_Punto : Catastro_Registro_Nucleo_V2_2_1.COL_DescripcionPuntoTipo;
      /** Corresponde a la exactitud vertical del punto lindero
       */
      Exactitud_Vertical : 0 .. 1000 [LADM_COL_V1_1.cm];
      /** Corresponde a la exactitud horizontal del punto lindero
       */
      Exactitud_Horizontal : MANDATORY 0 .. 1000 [LADM_COL_V1_1.cm];
      /** Indica si es o no fiable.
       */
      Confiabilidad : BOOLEAN;
      /** Se describe la posición del punto con relación a su estructura, si es un punto aislado, o si hace parte de un Arco o de una linea.
       */
      Posicion_Interpolacion (EXTENDED) : Catastro_Registro_Nucleo_V2_2_1.COL_InterpolacionTipo;
      /** Si se trata de un monumento geodésico, el tipo.
       */
      Monumentacion (EXTENDED) : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_MonumentacionTipo;
      /** Nombre o codigo del punto lindero
       */
      Nombre_Punto : TEXT*10;
    END PuntoLindero;

    /** Porción de tierra con una extensión geográfica definida.
     */
    CLASS Terreno
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_UnidadEspacial =
      /** Área del predio que se encuentra inscrita en el Folio de Matricula Inmobiliaria
       */
      Area_Registral : 0.0 .. 99999999999999.9 [LADM_COL_V1_1.m2];
      /** Área de predio resultado de los calculos realizados en el proceso de levantamiento planimetrico
       */
      Area_Calculada : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V1_1.m2];
      /** Valor asignado en el proceso de valoración economica masiva al terreno del predio
       */
      Avaluo_Terreno : MANDATORY LADM_COL_V1_1.LADM_Nucleo.Peso;
      /** se caracterizan los diferentes tipos de cultivos o territorios agricolas que conforman el predio, corresponde a la pregunta 5.3 del anexo 5.1 de los estandares de catastro multiproposito versión 2.1.1
       */
      Territorio_Agricola : BAG {0..*} OF Catastro_Registro_Nucleo_V2_2_1.COL_TerritorioAgricola_Terreno_Territorio_Agricola;
      /** Se describe si en el predio existe presencia de bosques o áreas seminaturales, corresponde a la pregunta 5.4 del anexo 5.1 de los estandares de catastro multiproposito versión 2.1.1
       */
      Bosque_Area_Seminaturale : BAG {0..*} OF Catastro_Registro_Nucleo_V2_2_1.COL_BosqueAreaSemi_Terreno_Bosque_Area_Seminaturale;
      /** En esta clase se identifican los valores de la pregunta 5.5. Especifique si evidencia en el terreno del Anexo 5, Versión 2.1.1 de Catastro Multiproposito
       */
      Evidencia_Cuerpo_Agua : BAG {0..*} OF Catastro_Registro_Nucleo_V2_2_1.COL_CuerpoAgua_Terreno_Evidencia_Cuerpo_Agua;
      /** Se caracteriza si en el predio existe algún tipo de explotación, corresponde a la pregunta 5.6 del anexo 5.1 de los estandares de catastro multiproposito versión 2.1.1
       */
      Explotacion : BAG {0..*} OF Catastro_Registro_Nucleo_V2_2_1.COL_ExplotacionTipo_Terreno_Explotacion;
      /** Se describe si en el predio existe alguna afectación natural de tipo inundación o de remoción en masa, corresponde a la pregunta 5.7 del anexo 5.1 de los estandares de catastro multiproposito versión 2.1.1
       */
      Afectacion : BAG {0..*} OF Catastro_Registro_Nucleo_V2_2_1.COL_Afectacion_Terreno_Afectacion;
      /** Tipo de derecho que limita el dominio de una porción del predio, corresponde a la pregunta 5.8 del anexo 5.1 de los estandares de catastro multiproposito versión 2.1.1
       */
      Servidumbre : BAG {0..*} OF Catastro_Registro_Nucleo_V2_2_1.COL_ServidumbreTipo_Terreno_Servidumbre;
      /** Corresponde a la figura geometrica vectorial poligonal, generada a partir de los linderos del predio.
       */
      poligono_creado (EXTENDED) : MANDATORY ISO19107_V1_MAGNABOG.GM_MultiSurface3D;
      !!@name="Area calculada"
      !!@ilivalid.msg="El area calculada debe ser mayor a cero ({t_id}) {su_Espacio_De_Nombres} - {su_Local_Id} "
      SET CONSTRAINT Area_Calculada > 0;
      !!@name="Avaluo terreno"
      !!@ilivalid.msg="El Avaluo terreno debe ser mayor a cero"
      SET CONSTRAINT Avaluo_Terreno > 0;
    END Terreno;

    /** Restricciones a las que está sometido un predio y que inciden sobre los derechos que pueden ejercerse sobre él.
     */
    CLASS COL_Restriccion
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_RRR =
      /** Indica si es preciso o no que un interesado esté asociado a la restricción.
       */
      Interesado_Requerido : BOOLEAN;
      /** Define el tipo de restricción.
       */
      Tipo : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_RestriccionTipo;
      /** Código con el que la responsabilidad se registra en el Registro de la Propiedad.
       */
      Codigo_Registral_Restriccion : TEXT*5;
    END COL_Restriccion;

    /** Clase especializada de LA_Punto que representa puntos demarcados que representan la posición horizontal de un vértice de construcción, servidumbre o auxiliare.
     */
    CLASS PuntoLevantamiento
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_Punto =
      /** Se caracterizan los diferentes tipos de punto levantamiento, estos son punto de construccción, punto de servidumbre o punto auxiliar
       */
      Tipo_Punto_Levantamiento : Catastro_Registro_Nucleo_V2_2_1.COL_PuntoLevTipo;
      /** Se caracteriza si el punto de levantamiento corresponde a un punto bien definido o no bien definido
       */
      Definicion_Punto : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_DefPuntoTipo;
      /** Corresponde a la exactitud vertical del punto levantamiento
       */
      Exactitud_Vertical : 0 .. 1000 [LADM_COL_V1_1.cm];
      /** Corresponde a la exactitud horizontal del punto levantamiento
       */
      Exactitud_Horizontal : 0 .. 1000 [LADM_COL_V1_1.cm];
      /** Se describe la posición del punto con relación a su estructura, si es un punto aislado, o si hace parte de un Arco o de una linea.
       */
      Posicion_Interpolacion (EXTENDED) : Catastro_Registro_Nucleo_V2_2_1.COL_InterpolacionTipo;
      /** Definición sobre si el punto de levantamiento se encuentra monumentado o no y los tipos de monumentación especificos para el perfil nacional
       */
      Monumentacion (EXTENDED) : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_MonumentacionTipo;
      /** Nombre que recibe el punto.
       */
      Nombre_Punto : TEXT*10;
    END PuntoLevantamiento;

    ASSOCIATION InteresadoContacto =
      contacto -- {0..*} Interesado_Contacto;
      interesado -- {1} COL_Interesado;
    END InteresadoContacto;

    ASSOCIATION predio_copropiedad =
      predio -- {0..*} Predio;
      copropiedad -<> {0..1} Predio;
      coeficiente : LADM_COL_V1_1.LADM_Nucleo.Fraccion;
    END predio_copropiedad;

    ASSOCIATION PublicidadBAUnit =
      pubicidad -- {0..*} Publicidad;
      baunit (EXTERNAL) -- {1} LADM_COL_V1_1.LADM_Nucleo.LA_BAUnit;
    END PublicidadBAUnit;

    ASSOCIATION PublicidadFuente =
      publicidad -- {0..1} Publicidad;
      fuente (EXTERNAL) -- {1..*} LADM_COL_V1_1.LADM_Nucleo.COL_FuenteAdministrativa;
    END PublicidadFuente;

    ASSOCIATION PublicidadInteresado =
      publicidad -- {0..*} Publicidad;
      interesado (EXTERNAL) -- {0..1} LADM_COL_V1_1.LADM_Nucleo.LA_Interesado;
    END PublicidadInteresado;

    /** Clase de tipo LA_RRR que registra las responsabilidades que las instancias de los interesados tienen sobre los predios.
     */
    CLASS COL_Responsabilidad
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_RRR =
      /** Definición del tipo de responsabilidad que se tiene.
       */
      Tipo : MANDATORY Catastro_Registro_Nucleo_V2_2_1.COL_ResponsabilidadTipo;
      /** Código con el que la responsabilidad se registra en el Registro de la Propiedad.
       */
      Codigo_Registral_Responsabilidad : TEXT*5;
    END COL_Responsabilidad;

    /** Tipo de unidad espacial que permite la representación de una servidumbre de paso asociada a una LA_BAUnit.
     */
    CLASS ServidumbrePaso
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_UnidadEspacial =
      Identificador : MANDATORY TEXT*20;
      /** Fecha de inscripción de la servidumbre en el Catastro.
       */
      Fecha_Inscripcion_Catastral : INTERLIS.XMLDate;
    END ServidumbrePaso;

    /** Clase que representa un tipo de restricción heredando de COL_Restricción, asociada a un derecho y que permite gestionar las hipotecas constituídas sobre un bien inmueble, considerando las cuestiones legales nacionales.
     */
    CLASS COL_Hipoteca
    EXTENDS COL_Restriccion =
      /** Tipo de hipoteca constituida, conforme a la legislación colombiana.
       */
      h_Tipo : Catastro_Registro_Nucleo_V2_2_1.COL_HipotecaTipo;
      /** Código con el que la hipoteca se registra en el Registro de la Propiedad Inmobiliaria en el momento de ser constituida.
       */
      Codigo_Registral_Hipoteca : TEXT*5;
    END COL_Hipoteca;

    /** Es cada conjunto de materiales consolidados dentro de un predio que tiene una caracteristicas especificas en cuanto a elementos constitutivos físicos y usos de los mismos.
     */
    CLASS UnidadConstruccion
    EXTENDS LADM_COL_V1_1.LADM_Nucleo.LA_EspacioJuridicoUnidadEdificacion =
      /** Corresponde al valor catastral determinado mediante el metodo economico definido, para cada unidad de contrucción del predio
       */
      Avaluo_Unidad_Construccion : LADM_COL_V1_1.LADM_Nucleo.Peso;
      /** Número de pisos que constituyen la unidad de construcción.
       */
      Numero_Pisos : MANDATORY 1 .. 100;
      /** Area de la unidad de contrucción.
       */
      Area_Construida : 0.0 .. 99999999999999.9 [LADM_COL_V1_1.m2];
      /** Área privada de la unidad de construcción para el caso en que las construcciones tienen regimen de propiedad horizontal.
       */
      Area_Privada_Construida : 0.0 .. 99999999999999.9 [LADM_COL_V1_1.m2];
    END UnidadConstruccion;

    ASSOCIATION ConstruccionUnidadConstruccion =
      unidadconstruccion -- {0..*} UnidadConstruccion;
      construccion -<> {1} Construccion;
    END ConstruccionUnidadConstruccion;

    ASSOCIATION hipotecaDerecho =
      hipoteca -- {0..*} COL_Hipoteca;
      derecho -- {0..*} COL_Derecho;
    END hipotecaDerecho;

  END Catastro_Registro;

END Catastro_Registro_Nucleo_V2_2_1.
